<h1>Hosted Events List:</h1>
<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>Date</th>
      <th>Location</th>
    </tr>
  </thead>
  <tbody>
    <%= @user.created_events do |event| %>
      <tr>
        <td><%=event.eventname%></td>
        <td><%=event.location%></td>
        <td><%=event.date%></td>
        <td><%=event.time%></td>
      </tr>
    <% end %>
  </tbody>
</table>